.PHONY: clean All

All:
	@echo "----------Building project:[ constructorClass - Debug ]----------"
	@cd "constructorClass" && "$(MAKE)" -f  "constructorClass.mk"
clean:
	@echo "----------Cleaning project:[ constructorClass - Debug ]----------"
	@cd "constructorClass" && "$(MAKE)" -f  "constructorClass.mk" clean
